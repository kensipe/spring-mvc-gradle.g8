<decorators defaultdir="/WEB-INF/jsp/decorators">

    <!--if some pages to be excluded from decoration-->
    <excludes>
        <!--<pattern>/dialog/</pattern>-->
    </excludes>

    <decorator name="catalog" page="$name$_master.jsp">
          <pattern>*/$name$/*</pattern>
    </decorator>

    <decorator name="master" page="master.jsp">
          <pattern>/*</pattern>
    </decorator>

</decorators>
